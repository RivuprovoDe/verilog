#! /usr/local/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
:vpi_module "/usr/local/lib/ivl/v2009.vpi";
S_0x25b08250 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x25b083e0 .scope module, "tb_half_str" "tb_half_str" 3 4;
 .timescale -8 -9;
v0x25b1c970_0 .var "a", 0 0;
v0x25b1ca30_0 .var "b", 0 0;
v0x25b1cb00_0 .net "c", 0 0, v0x25b1c730_0;  1 drivers
v0x25b1cc00_0 .net "s", 0 0, v0x25b1c800_0;  1 drivers
S_0x25ae3580 .scope module, "uut" "half_str" 3 9, 4 3 0, S_0x25b083e0;
 .timescale -8 -9;
    .port_info 0 /OUTPUT 1 "s";
    .port_info 1 /OUTPUT 1 "c";
    .port_info 2 /INPUT 1 "a";
    .port_info 3 /INPUT 1 "b";
v0x25b08860_0 .net "a", 0 0, v0x25b1c970_0;  1 drivers
v0x25b1c670_0 .net "b", 0 0, v0x25b1ca30_0;  1 drivers
v0x25b1c730_0 .var "c", 0 0;
v0x25b1c800_0 .var "s", 0 0;
E_0x25ae37d0 .event anyedge, v0x25b08860_0, v0x25b1c670_0;
    .scope S_0x25ae3580;
T_0 ;
    %wait E_0x25ae37d0;
    %load/vec4 v0x25b08860_0;
    %load/vec4 v0x25b1c670_0;
    %xor;
    %store/vec4 v0x25b1c800_0, 0, 1;
    %load/vec4 v0x25b08860_0;
    %load/vec4 v0x25b1c670_0;
    %and;
    %store/vec4 v0x25b1c730_0, 0, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x25b083e0;
T_1 ;
    %vpi_call/w 3 16 "$dumpfile", "tb_half_str.vcd" {0 0 0};
    %vpi_call/w 3 17 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x25ae3580 {0 0 0};
    %vpi_call/w 3 18 "$display", "a b | s c | Expected s c" {0 0 0};
    %vpi_call/w 3 19 "$display", "------------------------" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x25b1c970_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x25b1ca30_0, 0, 1;
    %delay 100, 0;
    %vpi_call/w 3 22 "$display", "%b %b | %b %b |    0 0", v0x25b1c970_0, v0x25b1ca30_0, v0x25b1cc00_0, v0x25b1cb00_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x25b1c970_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x25b1ca30_0, 0, 1;
    %delay 100, 0;
    %vpi_call/w 3 25 "$display", "%b %b | %b %b |    1 0", v0x25b1c970_0, v0x25b1ca30_0, v0x25b1cc00_0, v0x25b1cb00_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x25b1c970_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x25b1ca30_0, 0, 1;
    %delay 100, 0;
    %vpi_call/w 3 28 "$display", "%b %b | %b %b |    1 0", v0x25b1c970_0, v0x25b1ca30_0, v0x25b1cc00_0, v0x25b1cb00_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x25b1c970_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x25b1ca30_0, 0, 1;
    %delay 100, 0;
    %vpi_call/w 3 31 "$display", "%b %b | %b %b |    0 1", v0x25b1c970_0, v0x25b1ca30_0, v0x25b1cc00_0, v0x25b1cb00_0 {0 0 0};
    %vpi_call/w 3 33 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "-";
    "testbench.sv";
    "design.sv";
