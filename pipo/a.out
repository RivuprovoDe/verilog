#! /usr/local/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
:vpi_module "/usr/local/lib/ivl/v2009.vpi";
S_0x8550b50 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x8550ce0 .scope module, "tb_pipo" "tb_pipo" 3 2;
 .timescale -9 -12;
v0x8565380_0 .var "clk", 0 0;
v0x8565470_0 .var "d", 3 0;
v0x8565540_0 .net "q", 3 0, v0x8565120_0;  1 drivers
v0x8565640_0 .var "rst", 0 0;
S_0x852b580 .scope module, "uut" "pipo" 3 8, 4 2 0, S_0x8550ce0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 4 "d";
    .port_info 3 /OUTPUT 4 "q";
P_0x852b760 .param/l "N" 0 4 2, +C4<00000000000000000000000000000100>;
v0x8550e70_0 .net "clk", 0 0, v0x8565380_0;  1 drivers
v0x8565040_0 .net "d", 3 0, v0x8565470_0;  1 drivers
v0x8565120_0 .var "q", 3 0;
v0x8565210_0 .net "rst", 0 0, v0x8565640_0;  1 drivers
E_0x8518bf0 .event posedge, v0x8565210_0, v0x8550e70_0;
    .scope S_0x852b580;
T_0 ;
    %wait E_0x8518bf0;
    %load/vec4 v0x8565210_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x8565120_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x8565040_0;
    %assign/vec4 v0x8565120_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x8550ce0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x8565380_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x8565380_0;
    %inv;
    %store/vec4 v0x8565380_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x8550ce0;
T_2 ;
    %vpi_call/w 3 16 "$dumpfile", "tb_pipo.vcd" {0 0 0};
    %vpi_call/w 3 17 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x852b580 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x8565640_0, 0, 1;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x8565470_0, 0, 4;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x8565640_0, 0, 1;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x8565470_0, 0, 4;
    %delay 10000, 0;
    %pushi/vec4 12, 0, 4;
    %store/vec4 v0x8565470_0, 0, 4;
    %delay 10000, 0;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x8565470_0, 0, 4;
    %delay 10000, 0;
    %delay 20000, 0;
    %vpi_call/w 3 26 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x8550ce0;
T_3 ;
    %vpi_call/w 3 29 "$monitor", "time=%0t | clk=%b rst=%b d=%b q=%b", $time, v0x8565380_0, v0x8565640_0, v0x8565470_0, v0x8565540_0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "-";
    "testbench.sv";
    "design.sv";
