:vpi_module "/usr/local/lib/ivl/va_math.vpi";
:vpi_module "/usr/local/lib/ivl/v2009.vpi";
S_0xae8f8a0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0xae8fa30 .scope module, "tb_adder16" "tb_adder16" 3 2;
 .timescale -9 -12;
v0xaea49d0_0 .var "a", 15 0;
v0xaea4ab0_0 .var "b", 15 0;
v0xaea4b80_0 .var "cin", 0 0;
v0xaea4c80_0 .net "cout", 0 0, v0xaea4740_0;  1 drivers
v0xaea4d50_0 .net "sum", 15 0, v0xaea4800_0;  1 drivers
S_0xae6a580 .scope module, "uut" "adder16" 3 10, 4 2 0, S_0xae8fa30;
 .timescale -9 -12;
    .port_info 0 /INPUT 16 "a";
    .port_info 1 /INPUT 16 "b";
    .port_info 2 /INPUT 1 "cin";
    .port_info 3 /OUTPUT 16 "sum";
    .port_info 4 /OUTPUT 1 "cout";
v0xae90610_0 .net "a", 15 0, v0xaea49d0_0;  1 drivers
v0xaea4590_0 .net "b", 15 0, v0xaea4ab0_0;  1 drivers
v0xaea4670_0 .net "cin", 0 0, v0xaea4b80_0;  1 drivers
v0xaea4740_0 .var "cout", 0 0;
v0xaea4800_0 .var "sum", 15 0;
E_0xae6a760 .event anyedge, v0xae90610_0, v0xaea4590_0, v0xaea4670_0;
    .scope S_0xae6a580;
T_0 ;
    %wait E_0xae6a760;
    %load/vec4 v0xae90610_0;
    %pad/u 17;
    %load/vec4 v0xaea4590_0;
    %pad/u 17;
    %add;
    %load/vec4 v0xaea4670_0;
    %pad/u 17;
    %add;
    %split/vec4 16;
    %store/vec4 v0xaea4800_0, 0, 16;
    %store/vec4 v0xaea4740_0, 0, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0xae8fa30;
T_1 ;
    %vpi_call/w 3 19 "$display", "       a        b   cin  |      sum     cout" {0 0 0};
    %vpi_call/w 3 20 "$display", "--------------------------------------------" {0 0 0};
    %pushi/vec4 1, 0, 16;
    %store/vec4 v0xaea49d0_0, 0, 16;
    %pushi/vec4 1, 0, 16;
    %store/vec4 v0xaea4ab0_0, 0, 16;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xaea4b80_0, 0, 1;
    %delay 5000, 0;
    %vpi_call/w 3 24 "$display", "%h  %h   %b   |   %h    %b", v0xaea49d0_0, v0xaea4ab0_0, v0xaea4b80_0, v0xaea4d50_0, v0xaea4c80_0 {0 0 0};
    %pushi/vec4 65535, 0, 16;
    %store/vec4 v0xaea49d0_0, 0, 16;
    %pushi/vec4 1, 0, 16;
    %store/vec4 v0xaea4ab0_0, 0, 16;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0xaea4b80_0, 0, 1;
    %delay 5000, 0;
    %vpi_call/w 3 27 "$display", "%h  %h   %b   |   %h    %b", v0xaea49d0_0, v0xaea4ab0_0, v0xaea4b80_0, v0xaea4d50_0, v0xaea4c80_0 {0 0 0};
    %pushi/vec4 4660, 0, 16;
    %store/vec4 v0xaea49d0_0, 0, 16;
    %pushi/vec4 17185, 0, 16;
    %store/vec4 v0xaea4ab0_0, 0, 16;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0xaea4b80_0, 0, 1;
    %delay 5000, 0;
    %vpi_call/w 3 30 "$display", "%h  %h   %b   |   %h    %b", v0xaea49d0_0, v0xaea4ab0_0, v0xaea4b80_0, v0xaea4d50_0, v0xaea4c80_0 {0 0 0};
    %vpi_call/w 3 32 "$finish" {0 0 0};
    %end;
    .thread T_1;
